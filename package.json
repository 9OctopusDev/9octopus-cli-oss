{
	"name": "@9octopus/octopus-cli",
	"version": "0.1.1",
	"description": "A powerful, open-source AI coding agent for your terminal. Connect directly to LLMs or use our backend service.",
	"keywords": [
		"ai",
		"cli",
		"agent",
		"llm",
		"openai",
		"anthropic",
		"google",
		"gemini",
		"grok",
		"langchain",
		"langgraph",
		"coding-assistant",
		"terminal"
	],
	"homepage": "https://github.com/9OctopusDev/9octopus-cli-oss#readme",
	"bugs": {
		"url": "https://github.com/9OctopusDev/9octopus-cli-oss/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/9OctopusDev/9octopus-cli-oss.git"
	},
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc && node scripts/inject-env.js",
		"dev": "tsc --watch",
		"check": "prettier --check",
		"lint": "xo",
		"test": "npx ava",
		"fix": "prettier --write"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@auth0/auth0-spa-js": "^2.4.1",
		"@inkkit/ink-markdown": "^1.0.0",
		"@langchain/anthropic": "^1.1.3",
		"@langchain/core": "^1.1.0",
		"@langchain/google-genai": "^2.0.1",
		"@langchain/groq": "^1.0.2",
		"@langchain/langgraph": "^1.0.2",
		"@langchain/openai": "^1.1.3",
		"@types/eventsource": "^1.1.15",
		"cli-spinners": "^3.2.0",
		"dedent": "^1.6.0",
		"eventsource": "^4.0.0",
		"find-up": "^8.0.0",
		"ink": "^6.5.1",
		"ink-markdown": "^1.0.4",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-task-list": "^2.0.0",
		"ink-text-input": "^6.0.0",
		"js-yaml": "^4.1.0",
		"keytar": "^7.9.0",
		"marked": "^9.1.6",
		"marked-terminal": "^7.3.0",
		"meow": "^11.0.0",
		"node-fetch": "^3.3.2",
		"open": "^10.2.0",
		"react": "^19.0.2",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^19.0.2",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"dotenv": "^17.2.3",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"replace-in-file": "^8.3.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"files": [
			"dist/test/**/*.test.js"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off",
			"unicorn/expiring-todo-comments": "off"
		},
		"ignores": [
			"scripts/**",
			"test-ts-node.ts"
		]
	},
	"prettier": "@vdemedes/prettier-config"
}
